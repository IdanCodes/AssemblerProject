.extern width
.extern height
.entry matChar
.entry newLine
.define start = 1
matChar: .string "0"
spaceChar: .string " "
newLine: .data 10, 13, 0
i: .data start
j: .data start
printRow: mov #start, i
printChar: prn matChar
prn spaceChar
inc i[0]
cmp i[0], width[0]
bne printRow
prn newLine
inc j[0]
cmp j[0], height[0]
bne printRow
